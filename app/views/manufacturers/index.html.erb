<h1>Manufacturers</h1>

<ul>
  <% @manufacturers.each do |manufacturer| %>
    <li>
      <p><%= manufacturer.name %></p>
      <p><%= manufacturer.country %></p>
    </li>
  <% end %>
</ul>

<p><%= link_to 'Add a Manufacturer', new_manufacturer_path %></p>
<p><%= link_to 'See Available Cars', cars_path %></p>
